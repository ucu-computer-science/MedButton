    "configurations": [
        {
            "name": "MTB",
            "compilerPath": "${config:modustoolbox.toolsPath}/gcc/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "${mtbCompilerArgs}"
            ],
            "compileCommands": "./build/compile_commands.json",
            "intelliSenseMode": "gcc-x86",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
                "${mtbDefaultIncludePath}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "${mtbDefaultDefines}"
            ]
        }
